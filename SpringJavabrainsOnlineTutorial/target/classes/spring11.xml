<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0/EN" "http://springframework.org/dtd/spring-beans-2.0.dtd">

<!--  Dette scope="prototype" eksempel kunne også være singleton(hvilket er default hvilket ville være fjollet 
 -->
<beans>
	<bean id="triangle" class="tutorial10BeanAutowiring.Triangle" scope="prototype">
	</bean>
	
	<bean id="pointA" class="tutorial7InjectingObjects.Point">
		<property name="x" value="0" />
		<property name="y" value ="0" />
	</bean>
	
	<bean id="pointB" class="tutorial7InjectingObjects.Point">
		<property name="x" value="-20" />
		<property name="y" value ="0" />
	</bean>
	
	<bean id="pointC" class="tutorial7InjectingObjects.Point">
		<property name="x" value="20" />
		<property name="y" value ="0" />
	</bean>
	
	
</beans>